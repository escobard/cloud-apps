{
  "name": "create-app",
  "version": "0.0.2",
  "description": "An enterprise platform boilerplate, forked from escobard/create-dapp",
  "main": "index.js",
  "dependencies": {
    "concurrently": "^4.1.0",
    "prettier": "^1.17.0"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "docker-compose -f ops/dev.yaml up",
    "dev-ngnix": "docker-compose -f ops/dev-nginx.yaml up",
    "test-api-unit": "docker-compose -f ops/api-test-unit.yaml up --exit-code-from api",
    "test-ui-unit": "",
    "test-ui-e2e-no-data": "docker-compose -f ops/ui-test-e2e.yaml up",
    "test-api-integration": "docker-compose -f ops/api-test-integration.yaml up --exit-code-from integration",
    "test-ui-e2e-with-data": "",
    "test": "npm run test-api-unit && npm run test-api-integration",
    "prod": "docker-compose -f ops/prod.yaml up",
    "clean": "docker system prune -a",
    "test-legacy": "concurrently --names \"UI,API\" \"npm run test --prefix ui\" \"npm run test --prefix api\" ",
    "dev-legacy": "concurrently --names \"UI,API\" \"npm run start --prefix ui\" \"npm run dev --prefix api\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escobard/create-dapp.git"
  },
  "keywords": [
    "react",
    "node",
    "postgres"
  ],
  "author": "Daniel Escobar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/escobard/create-dapp/issues"
  },
  "homepage": "https://github.com/escobard/create-dapp#readme"
}

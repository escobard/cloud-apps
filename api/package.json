{
  "name": "api",
  "version": "0.0.2",
  "description": "api",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "set environment=development&& nodemon -L index.js",
    "test": "nyc --reporter=text mocha \"./{,!(node_modules)/**/}*.test.js\"",
    "test-watch": "set PORT=5555 && nyc mocha --watch \"./{,!(node_modules)/**/}*.test.js\"",
    "docker-build": "docker build -t escobard/create-app-api:latest -f ops/Dockerfile .",
    "docker-start": "docker run --name api -p 4000:4000 -t escobard/create-app-api:latest",
    "docker-rm": "docker stop api && docker rm api",
    "docker-test-build": "docker build -t escobard/create-app-api-test:latest -f ops/Dockerfile.test .",
    "docker-test-start": "docker run --name api_test -p 4000:4000 -t escobard/create-app-api-test:latest",
    "docker-test-rm": "docker stop api_test && docker rm api_test",
    "docker": "npm run docker-build && npm run docker-start",
    "docker-test": "npm run docker-test-build && npm run docker-test-start"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "author": "escobard",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "pg": "^7.4.3",
    "sequelize": "^5.21.2",
    "swagger-express-middleware": "^2.0.5",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sinon": "^9.0.0",
    "supertest": "^3.4.2"
  }
}
